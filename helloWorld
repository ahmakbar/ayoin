#include <iostream>
using namespace std;

int main(){

  cout<<"Hello World";
  cout<<"iyaa halo kamu";

  
  or (int a = 1; a < 10; a++){
		for (int b = 10; b >= a; b--){
			for (int c = 10; c >= b; c--){
				cout<<c;
			}
			cout<<endl;
		}
		cout<<endl;
	}

  cout<<"H A"<<endl;

  cout<<"Hello World"<<endl;

  cout<<"ngopi slurr"<<endl;
  cout<<"iyain aja";
  cout<<"iya";
  
  cout<<"Daleeemmmm"<<endl;
  cout<<"Daleeemmmm";
  cout<<"PeTIK Bismillah";

}
